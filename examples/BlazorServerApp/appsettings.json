{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "UserProfilePage": {
      "EnabledFor": [
        {
          "Name": "Auth",
          "Parameters": {}
        }
      ]
    },
    "VipContentEnhancement": {
      "EnabledFor": [
        {
          "Name": "Targeting",
          "Parameters": {
            "Audience": {
              "Users": [
                "admin"
              ],
              "Groups": [
                "vip"
              ],
              "DefaultRolloutPercentage": 0
            }
          }
        }
      ]
    }
  }
}
